{
  "name": "colorfa11y",
  "private": true,
  "description": "A simple web utility to easily convert between HEX, RGB, and HSL colors and determine WCAG color contrast ratio",
  "version": "0.1.0",
  "author": "David Evans",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json}",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest",
    "coverage": "yarn test --coverage",
    "cy": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint"
    ],
    "*.{js,ts,tsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "gatsby": "^2.15.1",
    "gatsby-image": "^2.2.15",
    "gatsby-plugin-manifest": "^2.2.12",
    "gatsby-plugin-offline": "^3.0.0",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-typescript": "^2.1.15",
    "gatsby-source-filesystem": "^2.1.18",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^5.0.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cypress": "^3.4.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gatsby-plugin-styled-components": "^3.3.2",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-styled-components": "^7.0.2",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidexns/colorfa11y"
  },
  "bugs": {
    "url": "https://github.com/davidexns/colorfa11y/issues"
  }
}
